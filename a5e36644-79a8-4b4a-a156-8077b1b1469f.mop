        "",
        "namespace Framefield.Core.ID6f9efb4a_2053_4031_baf4_a2148e78920e",
        "{",
        "    public class Class_SelectFromStrings : OperatorPart.Function",
        "    {",
        "        //>>> _inputids",
        "        private enum InputId",
        "        {",
