        "            if (i < 0)",
        "                i = -i;",
        "            var v1Input = ValuesNode.Connections[i];",
        "            string v1 = v1Input.Eval(context).Text;",
        "            ",
        "            context.Text = v1;",
        "",
        "            return context;",
        "        }",
