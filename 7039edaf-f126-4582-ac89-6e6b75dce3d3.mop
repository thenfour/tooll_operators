        "",
        "namespace Framefield.Core.ID54ca23c6_157c_4e00_8197_6230c3c7b164",
        "{",
        "    public class Class_SKServerBeat : OperatorPart.Function, Framefield.Core.OperatorPartTraits.ITimeAccessor",
        "    {",
        "        //>>> _inputids",
        "        private enum InputId",
        "        {",
        "            BeatPeriod = 0,",
        "            Offset = 1",
        "        }",
        "        //<<< _inputids",
        "        public override OperatorPartContext Eval(OperatorPartContext context, List<OperatorPart> inputs, int outputIdx) ",
