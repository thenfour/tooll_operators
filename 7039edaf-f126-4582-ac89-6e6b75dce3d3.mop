        "            // >>> _params",
        "            var BeatPeriod = inputs[(int)InputId.BeatPeriod].Eval(context).Value;",
        "            var Offset = inputs[(int)InputId.Offset].Eval(context).Value;",
        "            // <<< _params",
        "            //>>> function",
        "            var skstate = Framefield.Core.OperatorPartContext.SKServerState;",
        "                context.Value = (float)((skstate.ProgramStreamBeat + Offset) / BeatPeriod);",
        "            return context;",
        "            ",
        "            //<<< function",
