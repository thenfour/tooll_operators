        "//<<< _using",
        "",
        "namespace Framefield.Core.ID07447280_1d33_4719_9be5_c54f6b79c126",
        "{",
        "    public class Class_RasterBars : FXImageFunction",
        "    {",
        "        //>>> _inputids",
        "        private enum InputId",
        "        {",
        "            Code = 0,",
        "            BarCount = 1,",
        "            MarginTop = 2,",
        "            MarginBottom = 3,",
        "            MarginLeft = 4,",
        "            MarginRight = 5,",
        "            WidthRandLeft = 6,",
        "            WidthRandRight = 7,",
        "            RandomSeedWidth = 8,",
        "            RandomSeedColor = 9",
        "        }",
        "        //<<< _inputids",
        "        public override OperatorPartContext Eval(OperatorPartContext context, List<OperatorPart> inputs, int outputIdx) {",
        "",
        "            //>>> _params",
        "            var Code = inputs[(int)InputId.Code].Eval(context).Text;",
        "            var BarCount = inputs[(int)InputId.BarCount].Eval(context).Value;",
        "            var MarginTop = inputs[(int)InputId.MarginTop].Eval(context).Value;",
        "            var MarginBottom = inputs[(int)InputId.MarginBottom].Eval(context).Value;",
        "            var MarginLeft = inputs[(int)InputId.MarginLeft].Eval(context).Value;",
        "            var MarginRight = inputs[(int)InputId.MarginRight].Eval(context).Value;",
        "            var Margin = new Vector4(MarginTop, MarginBottom, MarginLeft, MarginRight);",
        "            var WidthRandLeft = inputs[(int)InputId.WidthRandLeft].Eval(context).Value;",
        "            var WidthRandRight = inputs[(int)InputId.WidthRandRight].Eval(context).Value;",
        "            var WidthRand = new Vector2(WidthRandLeft, WidthRandRight);",
        "            var RandomSeedWidth = inputs[(int)InputId.RandomSeedWidth].Eval(context).Value;",
        "            var RandomSeedColor = inputs[(int)InputId.RandomSeedColor].Eval(context).Value;",
        "            //<<< _params",
        "",
        "            return PrepareAndEvalOnChange(context, () => {",
        "                ClearRenderTarget(context, new SharpDX.Color4(0, 0, 0, 0));",
        "",
        "                //>>> _setup",
        "                {",
        "                    _effect.GetVariableByName(\"RenderTargetSize\").AsVector().Set(new Vector2(_usedViewport.Width, _usedViewport.Height));",
        "                    _effect.GetVariableByName(\"BarCount\").AsScalar().Set(BarCount);",
        "                    _effect.GetVariableByName(\"Margin\").AsVector().Set(new Vector4(MarginTop, MarginBottom, MarginLeft, MarginRight));",
        "                    _effect.GetVariableByName(\"WidthRand\").AsVector().Set(new Vector2(WidthRandLeft, WidthRandRight));",
        "                    _effect.GetVariableByName(\"RandomSeedWidth\").AsScalar().Set(RandomSeedWidth);",
        "                    _effect.GetVariableByName(\"RandomSeedColor\").AsScalar().Set(RandomSeedColor);",
        "                //<<< _setup",
        "",
        "                Render(context);",
        "",
        "                //>>> _cleanup",
        "                }",
        "                //<<< _cleanup",
        "            });",
