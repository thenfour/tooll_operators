      "DefaultValue": {
        "Type": "Text",
        "Value": [
          "//>>> _common parameters\r",
          "float4x4 objectToWorldMatrix;",
          "float4x4 worldToCameraMatrix;",
          "float4x4 projMatrix;",
          "Texture2D txDiffuse;",
          "float2 RenderTargetSize;",
          "//<<< _common parameters\r",
          "\r",
          "//>>> _parameters\r",
          "float BarCount;",
          "float4 Margin;",
          "float2 WidthRand;",
          "float RandomSeedWidth;",
          "float RandomSeedColor;",
          "//<<< _parameters\r",
          "\r",
          " \r",
          "SamplerState samPoint\r",
          "{\r",
          "    Filter = MIN_MAG_MIP_LINEAR;\r",
          "    AddressU = Wrap;\r",
          "    AddressV = Wrap;\r",
          "};\r",
          "\r",
          "struct VS_IN\r",
          "{\r",
          "    float4 pos : POSITION;\r",
          "    float2 texCoord : TEXCOORD;\r",
          "};\r",
          "\r",
          "struct PS_IN\r",
          "{\r",
          "    float4 pos : SV_POSITION;\r",
          "    float2 texCoord: TEXCOORD0;\r",
          "};\r",
          "\r",
          "//>>> _GS\r",
          "",
          "//<<< _GS\r",
          "\r",
          "//>>> _VS \r",
          "PS_IN VS( VS_IN input )",
          "{",
          "    PS_IN output = (PS_IN)0;",
          "",
          "    output.pos = mul(input.pos, worldToCameraMatrix);",
          "    output.pos = mul(output.pos, projMatrix);",
          "    output.texCoord = input.texCoord;",
          "",
          "    return output;",
          "}",
          "//<<< _VS\r",
          "\r",
          "//>>> PS\r",
          "float4 hash41(float p)\r",
          "{\r",
          "    p += 1e2;\r",
          "    p *= 8.222;\r",
          "\tfloat4 p4 = frac(p * float4(.1031, .1030, .0973, .1099));\r",
          "    p4 += dot(p4, p4.wzxy+33.33);\r",
          "    return frac((p4.xxyz+p4.yzzw)*p4.zywx);\r",
          "}\r",
          "\r",
          "float4 PS( PS_IN input ) : SV_Target\r",
          "{\r",
          "    float barHeight = 1./BarCount;\r",
          "    \r",
          "    float currentBarID = floor((input.texCoord.y) / barHeight);\r",
          "    float currentBarCenter = (currentBarID + .5) * barHeight;\r",
          "    float currentBarPos = frac(input.texCoord.y / barHeight);\r",
          "    float4 currentBarColor = hash41(currentBarID + RandomSeedColor);\r",
          "    float4 currentBarRandPos = hash41(currentBarID + RandomSeedWidth);\r",
          "    float4 nextBarColor = hash41(currentBarID + RandomSeedColor + 1);\r",
          "    \r",
          "    float a = 0;\r",
          "    \r",
          "    float marginLeft = Margin.z + (currentBarRandPos.x - .5) * WidthRand.x;\r",
          "    float marginRight = Margin.w + (currentBarRandPos.y - .5) * WidthRand.y;\r",
          "    \r",
          "    if (currentBarCenter < Margin.x) {\r",
          "        a = 1;// top\r",
          "    }\r",
          "    if (currentBarCenter > (1.-Margin.y)) {\r",
          "        a = 1; // bottom\r",
          "    }\r",
          "    if (input.texCoord.x < marginLeft) {\r",
          "        a = 1;// left\r",
          "    }\r",
          "    if (input.texCoord.x > (1.-marginRight)) {\r",
          "        a = 1; // right\r",
          "    }\r",
          "    \r",
          "\r",
          "    return float4(lerp(currentBarColor.rgb, nextBarColor.rgb, currentBarPos), a);\r",
          "}\r\r\r",
          "//<<< PS\r",
          "\r",
          "//>>> _technique\r",
          "technique10 Render",
          "{",
          "    pass P0",
          "    {",
          "        SetGeometryShader( 0 );",
          "        SetVertexShader( CompileShader( vs_4_0, VS() ) );",
          "        SetPixelShader( CompileShader( ps_4_0, PS() ) );",
          "    }",
          "}",
          "//<<< _technique\r",
          ""
        ]
      },
